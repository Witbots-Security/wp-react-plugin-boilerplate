{
   "name": "wp-webpack-example",
   "version": "1.0.0",
   "description": "Simple WordPress plugin that uses Webpack.",
   "main": "index.js",
   "babel": {
      "presets": [
         "@babel/preset-env",
         "@babel/preset-react"
      ]
   },
   "scripts": {
      "watch": "webpack --mode=development --watch --config webpack-config.js",
      "build": "webpack --mode=production --config webpack-config.js"
   },
   "keywords": [
      "wordpress",
      "webpack",
      "javascript"
   ],
   "author": "WebDevStudios",
   "license": "GPL-2.0+",
   "devDependencies": {
      "@babel/cli": "^7.1.5",
      "@babel/core": "^7.1.6",
      "@babel/preset-env": "^7.1.6",
      "@babel/preset-react": "^7.12.10",
      "babel-loader": "^8.0.4",
      "lodash": "^4.17.11",
      "webpack": "^4.26.1",
      "webpack-cli": "^3.1.2"
   },
   "dependencies": {
      "react": "^17.0.1",
      "react-dom": "^17.0.1"
   }
}
